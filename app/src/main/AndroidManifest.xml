<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.leothosthoren.moodtracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground_smiley_happy"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_smiley_happy_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controler.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controler.HistoryActivity" />

        <receiver
            android:name=".model.MoodAlarmReceiver"
            android:enabled="true"
            android:exported="true">
        </receiver>
    </application>

</manifest>